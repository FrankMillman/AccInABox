      <mem_col col_name="finrpt_data" data_type="JSON" short_descr="Report definition data"
        long_descr="Report definition data"/>
    </mem_obj>
  </mem_objects>
  <input_params>
    <input_param name="finrpt_data" type="data_attr" target="var.finrpt_data" required="true"/>
  </input_params>
  <output_params/>
  <frame main_object="finrpt_obj">
    <toolbar/>
    <body>
      <block/>
      <grid data_object="finrpt_obj" num_grid_rows="20">
        <toolbar template="Grid_Finrpt"/>
        <grid_methods template="Grid"/>
      </grid>
    </body>
    <button_row/>
    <frame_methods/>
  </frame>
</form>
