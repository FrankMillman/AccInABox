<form name="setup_params">
  <db_objects>
    <db_obj name="params" table_name="adm_params"/>
  </db_objects>
  <mem_objects/>
  <input_params/>
  <output_params/>
  <frame main_object="params" before_start_form="
      <<action>>
        <<assign>>
          <<source>>int(1)<</source>>
          <<target>>params.row_id<</target>>
        <</assign>>
      <</action>>
      ">
    <toolbar/>
    <body>
      <block/>
      <panel/>
      <row/>
      <col/>
      <label value="Company Id:"/>
      <col/>
      <input fld="params.company_id" lng="60"/>
      <row/>
      <col/>
      <label value="Company name:"/>
      <col/>
      <input fld="params.company_name" lng="160"/>
      <block/>
      <nb_start/>

      <nb_page label="Org"/>
      <block/>
      <panel/>
      <row/>
      <col/>
      <label value="Generate party id?"/>
      <col/>
      <input fld="params.auto_party_id" lng="60"/>
      <row/>
      <col/>
      <label value="Format for party id:"/>
      <col/>
      <input fld="params.party_id_format" lng="120"/>

      <nb_page label="Adm"/>
      <block/>
      <panel/>
      <row/>
      <col/>
      <label value="Local currency"/>
      <col/>
      <input fld="params.local_currency" lng="120"/>

      <nb_end/>

    </body>
    <button_row validate="true" template="Setup_Form"/>
    <frame_methods template="Setup_Form">
    </frame_methods>
  </frame>
</form>
